{
    "name": "jostkleigrewe/openai-core-bundle",
    "type": "symfony-bundle",
    "description": "OpenAICoreBundle for the Symfony Framework",
    "keywords": ["Jostkleigrewe", "OpenAI", "ChatGPT", "Symfony", "Bundle"],
    "homepage": "https://projects.jostkleigrewe.com/jostkleigrewe/openai-core-bundle/",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Jostkleigrewe",
            "email": "sven@jostkleigrewe.com"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "symfony/config": "^7.1",
        "symfony/framework-bundle": "^7.1",
        "symfony/http-client": "^7.1",
        "symfony/serializer": "^7.1"
    },
    "require-dev": {
        "symfony/test-pack": "dev-main"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "Jostkleigrewe\\OpenAiCoreBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Jostkleigrewe\\OpenAiCoreBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
        }
    },
    "scripts": {
        "phpunit": "php vendor/bin/phpunit",
        "phpunit-coverage": "XDEBUG_MODE=coverage php vendor/bin/phpunit --coverage-html=var/coverage"
    }
}
